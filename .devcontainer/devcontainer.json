{
    "name": "PermissionHub",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "server",
    "workspaceFolder": "/work",
    "extensions": [
        "ms-azuretools.vscode-docker",
        "github.copilot"
    ],
    "hostRequirements": {
        "cpus": 4,
        "memory": "8gb"
    },
    "portsAttributes": {
        "3000": {
            "label": "Client"
        },
        "3001": {
            "label": "Server"
        },
        "8080": {
            "label": "pgAdmin"
        }
    }
}
